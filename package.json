{
  "name": "moodcycle-monorepo",
  "version": "1.0.0",
  "description": "MoodCycle - Monorepo pour App, API et Admin",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "start": "npm run start --workspace=moodcycle",
    "start:api": "npm run dev --workspace=moodcycleapi",
    "start:admin": "npm run dev --workspace=moodcycle-admin",
    "start:all": "concurrently \"npm run start:api\" \"npm run start:admin\"",
    "build": "npm run build --workspaces --if-present",
    "test": "npm run test --workspaces --if-present",
    "clean": "npm run clean --workspaces --if-present && rm -rf node_modules",
    "install:all": "npm install",
    "dev:api": "npm run dev --workspace=moodcycleapi",
    "dev:app": "npm run start --workspace=moodcycle"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "author": "IrimWebForge",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/your-username/moodcycle.git"
  }
} 